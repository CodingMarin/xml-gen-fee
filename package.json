{
  "name": "xml-gen-fee",
  "version": "0.0.1",
  "description": "A library for generating XML invoices using Mustache templates.",
  "main": "./dist/index.js",
  "exports": {
    "types": "./dist/types.d.ts",
    "require": "./dist/index.js"
  },
  "directories": {
    "test": "./src/templates"
  },
  "scripts": {
    "build": "npx tsc && copyfiles -u 1 src/templates/**/* dist",
    "dev": "ts-node ./src/index.ts",
    "start": "node ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "XML Gen",
    "Boleta XML",
    "Generar Boleta XML"
  ],
  "author": "Alexander Marin <marinalexander691@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodingMarin/xml-gen-fee.git"
  },
  "bugs": {
    "url": "https://github.com/CodingMarin/xml-gen-fee/issues"
  },
  "dependencies": {
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.0.0",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
